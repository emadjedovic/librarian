<h2>Members</h2>
<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Zip Code</th>
      <th>Edit</th>
      <th>Delete</th>
    </tr>
  </thead>
  <tbody>
    <% members.forEach(member => { %>
    <tr>
      <td><a href="<%= `/members/${member._id}` %>"><%= member.fullName %></a></td>
      <td><%= member.email %></td>
      <td><%= member.zipCode %></td>
      <!--edit link tag-->
      <td>
        <a href="<%=`/members/${member._id}/edit` %>"> Edit </a>
      </td>
      <!--delete link tag-->
      <td>
        <a href="<%= `members/${member._id}/delete?_method=DELETE` %>"
          onclick="return confirm('Are you sure you want to delete this member?')">
          Delete
        </a>
      </td>
    </tr>
    <% }); %>
  </tbody>
</table>
